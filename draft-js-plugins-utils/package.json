{
  "name": "draft-js-plugins-utils",
  "version": "2.0.3",
  "description": "Plugin utilities for draft js",
  "author": {
    "name": "Julian Krispel-Smasel",
    "email": "julian@reactrocket.com"
  },
  "repository": {
